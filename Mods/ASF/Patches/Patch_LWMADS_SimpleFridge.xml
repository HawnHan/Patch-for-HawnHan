<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Simple Utilities: Fridge</li>
        </mods>
        <match Class="PatchOperationConditional">
            <xpath>Defs/ThingDef[defName="AdaptiveStorageBase"]</xpath>
            <match Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="OwlFridgeBase"]/building/maxItemsInCell</xpath>
                <value>
                    <maxItemsInCell>5</maxItemsInCell>
                </value>
            </match>
        </match>
    </Operation>
</Patch>