	<ThingDef ParentName="AdaptiveStorageBase">
		<defName>HawnHan_DeepStorage_Statue_ASF</defName>
		<label>雕像柜</label>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<graphicData>
			<texPath>Safe/Safe</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,3)</drawSize>
            <drawRotated>false</drawRotated>
            <shaderType>CutoutComplex</shaderType>
		</graphicData>
		<defaultPlacingRot>South</defaultPlacingRot>
		<uiIconPath>Safe/Safe_UI</uiIconPath>
		<uiIconScale>1</uiIconScale>
		<fillPercent>0.8</fillPercent>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>800</costStuffCount>
		<costList>
			<ComponentIndustrial>20</ComponentIndustrial>
			<ComponentSpacer>6</ComponentSpacer>
		</costList>
		<statBases>
			<Mass>1000</Mass>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>-4</Beauty>
		</statBases>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<size>(1,1)</size>
		<rotatable>true</rotatable>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<maxItemsInCell>80</maxItemsInCell>
			<fixedStorageSettings>
				<filter>
					<categories>
						<li>BuildingsArt</li>
					</categories>
				</filter>
			</fixedStorageSettings>
			<paintable>true</paintable>
		</building>
		<description>个人需求制作的雕像柜。可存放至多80个雕像</description>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
			<li>Machining</li>
		</researchPrerequisites>
	</ThingDef>
