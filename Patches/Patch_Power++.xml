<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--Power++ and Rimstro-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rimstro</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="LWM_DS_Rimstro_Refrigerator"]/comps</xpath>
			<value>
				<li Class="aRandomKiwi.PPP.CompProperties_LocalWirelessPowerReceptor">
					<compClass>aRandomKiwi.PPP.CompLocalWirelessPowerReceptor</compClass>
				</li>
			</value>
		</match>
	</Operation>
	<!--Power++ and Android tier-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Android tiers</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="AndroidOperationBed"]/comps</xpath>
			<value>
				<li Class="aRandomKiwi.PPP.CompProperties_LocalWirelessPowerReceptor">
					<compClass>aRandomKiwi.PPP.CompLocalWirelessPowerReceptor</compClass>
				</li>
			</value>
		</match>
	</Operation>
</Patch>