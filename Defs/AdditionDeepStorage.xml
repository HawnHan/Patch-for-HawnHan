<Defs>
	<ThingDef ParentName="LWM_DeepStorage" MayRequire="lwm.deepstorage">
		<defName>HawnHan_DeepStorage_Statue</defName>
		<label>雕像柜</label>
		<minifiedDef>MinifiedThing</minifiedDef>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<tradeTags Inhert="false" />
		<drawGUIOverlay>true</drawGUIOverlay>
		<graphicData>
			<texPath>sumghai/safe/safe</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<defaultPlacingRot>South</defaultPlacingRot>
		<uiIconPath>sumghai/safe/safe_UI</uiIconPath>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.8</fillPercent>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>800</costStuffCount>
		<costList>
			<ComponentIndustrial>20</ComponentIndustrial>
			<ComponentSpacer>6</ComponentSpacer>
		</costList>
		<statBases>
			<Mass>700</Mass>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Flammability>0</Flammability>
			<Beauty>-4</Beauty>
		</statBases>
		<size>(1,1)</size>
		<comps>
			<li Class="LWM.DeepStorage.Properties">
				<maxNumberStacks>80</maxNumberStacks>
				<maxMassOfStoredItem>2000</maxMassOfStoredItem>
				<minTimeStoringTakes>100</minTimeStoringTakes>
				<additionalTimeEachStack>35</additionalTimeEachStack>
				<additionalTimeEachDef>20</additionalTimeEachDef>
				<showContents>false</showContents>
				<overlayType>SumOfAllItems</overlayType>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<fixedStorageSettings>
				<priority>Important</priority>
				<filter>
					<categories>
						<li>BuildingsArt</li>
					</categories>
				</filter>
			</fixedStorageSettings>
		</building>
