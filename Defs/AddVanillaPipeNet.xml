<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--玻璃钢-->
	<!--先定义管道系统-->
	<PipeSystem.PipeNetDef MayRequire="oskarpotocki.vanillafactionsexpanded.core">
		<defName>Hawn_PlasteelNet</defName>
		<resource>
			<name>Plasteel</name>
			<unit>l</unit>
			<color>(0.70, 0.23, 0.23)</color>
			<onlyShowStored>True</onlyShowStored>
			<offTexPath>UI/Gizmos/OutOfDeepchem</offTexPath>
		</resource>
		<pipeDefs>
			<li>Hawn_PlasteelPipe</li>
			<li>Hawn_UndergroundPlasteelPipe</li>
		</pipeDefs>
		<designator>
			<deconstructIconPath>UI/Gizmos/DeepchemPipes_Deconstruct</deconstructIconPath>
			<designationCategoryDef>VCHE_PipeNetworks</designationCategoryDef>
		</designator>
		<overlayOptions>
			<overlayColor>(0.70, 0.23, 0.23)</overlayColor>
		</overlayOptions>
	</PipeSystem.PipeNetDef>
	<!--然后定义两种运输管道-->
	<ThingDef ParentName="BuildingBase" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
		<defName>Hawn_PlasteelPipe</defName>
		<label>玻璃钢管道</label>
		<description>沉重结实的钢制管道，可以将玻璃钢泵抽出的玻璃钢输送至贮存罐中。</description>
		<thingClass>PipeSystem.Building_Pipe</thingClass>
		<category>Building</category>
		<tickerType>Rare</tickerType>
		<graphicData>
			<texPath>Things/Building/Linked/DeepchemPipes_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom8</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Linked/DeepchemPipes_MenuIcon</uiIconPath>
		<uiIconScale>0.7</uiIconScale>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/DeepchemPipes_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<drawerType>MapMeshOnly</drawerType>
		<altitudeLayer>Conduits</altitudeLayer>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>40</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<costList>
			<Steel>4</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>Hawn_PlasteelNet</pipeNet>
			</li>
		</comps>
		<placeWorkers>
			<li>PipeSystem.PlaceWorker_Pipe</li>
		</placeWorkers>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>VCHE_PipeNetworks</designationCategory>
		<constructEffect>ConstructMetal</constructEffect>
		<clearBuildingArea>false</clearBuildingArea>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>
	<ThingDef ParentName="BuildingBase" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
		<defName>Hawn_UndergroundPlasteelPipe</defName>
		<label>地下玻璃钢管道</label>
		<description>沉重结实的钢制管道，可以将玻璃钢泵抽出的沉积原液输送至贮存罐中。这种管道既不会被看见也不会爆炸，但是造价比寻常管道更加昂贵。</description>
		<thingClass>PipeSystem.Building_Pipe</thingClass>
		<category>Building</category>
		<tickerType>Rare</tickerType>
		<graphicData>
			<texPath>UI/CSG/IConduit</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom8</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Linked/DeepchemPipes_MenuIcon</uiIconPath>
		<uiIconScale>0.7</uiIconScale>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/DeepchemPipes_Blueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<drawerType>MapMeshOnly</drawerType>
		<altitudeLayer>Conduits</altitudeLayer>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>120</WorkToBuild>
			<Flammability>0</Flammability>
		</statBases>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<costList>
			<Steel>6</Steel>
		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_Resource">
				<pipeNet>Hawn_PlasteelNet</pipeNet>
			</li>
		</comps>
		<placeWorkers>
			<li>PipeSystem.PlaceWorker_Pipe</li>
		</placeWorkers>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>VCHE_PipeNetworks</designationCategory>
		<constructEffect>ConstructMetal</constructEffect>
		<clearBuildingArea>false</clearBuildingArea>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
	</ThingDef>
	<!--因个人需求并不需要精炼及存储，因此跳过管道阀门及存储罐-->
	<ThingDef ParentName="BuildingBase" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
		<defName>Hawn_PlasteelPumpjack</defName>
		<label>玻璃钢泵</label>
		<description>一台用于抽取蕴藏在地下深处的玻璃钢的深钻井装置，可使用地质扫描仪来勘探资源的所在处。与深钻井一样可以卸载并重新安装。</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Building/Production/DeepchemPumpjack</texPath>
			<drawSize>(3,3)</drawSize>
			<drawRotated>false</drawRotated>
		</graphicData>
		<size>(3,3)</size>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<fillPercent>0.5</fillPercent>
		<designationCategory>VCHE_PipeNetworks</designationCategory>
		<rotatable>false</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<uiIconPath>Things/Building/Production/DeepchemPumpjack_MenuIcon</uiIconPath>
		<uiIconScale>1</uiIconScale>
		<minifiedDef>MinifiedThing</minifiedDef>
		<building>
			<uninstallWork>1800</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
		</building>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<costList>
			<Steel>150</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
			<ComponentSpacer>1</ComponentSpacer>
		</costList>
		<placeWorkers>
			<li>PipeSystem.PlaceWorker_DeepExtractor</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<researchPrerequisites>
			<li>BiofuelRefining</li>
		</researchPrerequisites>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>10000</WorkToBuild>
			<Flammability>1</Flammability>
			<Beauty>-25</Beauty>
			<Mass>70</Mass>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Flickable" />
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>300</basePowerConsumption>
			</li>
			<li Class="PipeSystem.CompProperties_DeepExtractor">
				<pipeNet>Hawn_PlasteelNet</pipeNet>
				<deepThing>Plasteel</deepThing>
				<ticksPerPortion>60</ticksPerPortion>
				<useDeepCountPerPortion>false</useDeepCountPerPortion>
				<soundAmbient>VCHE_PumpjackAmbient</soundAmbient>
			</li>
			<li Class="VCHE.CompProperties_PumpjackAnimation" />
		</comps>
		<modExtensions>
			<li Class="VFECore.ThingDefExtension">
				<deepResourcesOnGUI>true</deepResourcesOnGUI>
			</li>
		</modExtensions>
	</ThingDef>
	
